{
	"name": "example-service",
	"version": "1.0.0",
	"description": "",
	"license": "ISC",
	"author": "",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"build": "tsc --project tsconfig.build.json",
		"check:spelling": "cspell \"**\" ",
		"check:types": "tsc --noEmit",
		"check:unused": "knip",
		"dev": "nodemon --watch src --watch package.json --watch package-lock.json --exec \"node --loader ts-node/esm\" src/index.ts",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint src/**/*.ts",
		"lint:md": "markdownlint \"**/*.md\" --rules sentences-per-line",
		"start": "node dist/index.js",
		"test": "vitest run --passWithNoTests",
		"test:ui": "vitest --ui",
		"verify": "npm run lint && npm run lint:md && npm run format:check && npm run check:types && npm run check:unused && npm run check:spelling && npm run build && npm run test"
	},
	"dependencies": {},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
		"@eslint/js": "9.29.0",
		"@total-typescript/tsconfig": "1.0.4",
		"@types/eslint-plugin-markdown": "2.0.2",
		"@types/node": "22.13.10",
		"@vitest/coverage-v8": "3.2.4",
		"@vitest/eslint-plugin": "1.1.38",
		"@vitest/ui": "3.2.4",
		"console-fail-test": "0.5.0",
		"cspell": "8.17.5",
		"eslint": "9.29.0",
		"eslint-plugin-jsdoc": "50.6.8",
		"eslint-plugin-jsonc": "2.20.0",
		"eslint-plugin-markdown": "5.1.0",
		"eslint-plugin-n": "17.16.2",
		"eslint-plugin-package-json": "0.29.0",
		"eslint-plugin-perfectionist": "4.11.0",
		"eslint-plugin-regexp": "2.7.0",
		"eslint-plugin-yml": "1.17.0",
		"knip": "5.61.2",
		"markdownlint": "0.38.0",
		"markdownlint-cli": "0.45.0",
		"nodemon": "3.1.10",
		"prettier": "3.5.3",
		"prettier-plugin-curly": "0.3.2",
		"prettier-plugin-packagejson": "2.5.15",
		"prettier-plugin-sh": "0.17.4",
		"sentences-per-line": "0.3.0",
		"ts-node": "10.9.2",
		"typescript": "5.8.3",
		"typescript-eslint": "8.26.1",
		"vitest": "3.2.4"
	}
}
